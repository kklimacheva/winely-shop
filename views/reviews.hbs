<!DOCTYPE html>
<html lang="ru">
<head>
  <meta charset="utf-8">
  <meta name="keywords" content="wine, shop, blog, wine news">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Reviews</title>
  {{> staticResources }}
</head>
<body>
{{> navigation }}
<article>
  <button id="getReviewsButton" type="button">Загрузить отзывы</button>
  <div id="preloader"></div>
  <div id="reviews" class="reviews"></div>
  <template id="reviewItem">
    <div class="reviewItem">
      <h3></h3>
      <p></p>
      <div></div>
      <br>
    </div>
  </template>
</article>
<auth class="bg">
  <div class="box">
    <h1>Вход в личный кабинет</h1>
    <form action="">
      <input class="inp" type="text" placeholder="Логин">
      <input class="inp" type="password" placeholder="Пароль">
      <hr style="border-color: rgba(0,0,0, 0)">
      <input class="btn" type="button" value="Вход">
    </form>
  </div>
</auth>
{{> footer }}
{{ supertikensInit }}
</body>
</html>